             <if test="updateTime != null "> and update_time = #{updateTime}</if>
         </where>
    </select>
    
    <select id="selectWorkPipeById" parameterType="Integer" resultMap="WorkPipeResult">
        <include refid="selectWorkPipeVo"/>
