	<sql id="selectWorkPipeVo">
        select id, ship_code, batch_id, unit_id, area_code, segment_code, tray_code, assembly_code, set_code, call_ship_id, assembly_pipe_id, pipe_diameter, pipe_out_diameter, pipe_thickness, pipe_material, pipe_material_level, pipe_shape, surface_treat, surface_name, cut_length, outfield, update_time from pipe_work_pipe
